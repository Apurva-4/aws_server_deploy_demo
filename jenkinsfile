pipeline {
  agent {
    kubernetes {
      yaml '''
        apiVersion: v1
        kind: Pod
        spec:
          containers:
          - name: maven
            image: maven:alpine
            command:
            - cat
            tty: true
        '''
    }
  }
      stages{
        stage('test1'){
          steps {
            withKubeConfig([
              credentialsId: 'mykubeconfig',
              namespace: 'frontend',
              podName: 'httpd-deployment-859c98485f-rx6ml'
            ]) {
                 sh 'kubectl exec httpd-deployment-859c98485f-rx6ml -n frontend'
            }
          }
        }
      }
    }



